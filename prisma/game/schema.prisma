generator client {
  provider = "prisma-client-js"
  output = "./generated/gameDataClient"
  binaryTargets = ["native", "debian-openssl-3.0.x"]
}


datasource gamedb {
  provider = "mysql"
  url      = env("DATABASE_GAME_URL")
}

model player {
  player_id       Int                 @id
  name            String  // 선수명
  speed           Int     // 이속
  goal_desicion   Int     // 골결
  shoot_power     Int     // 슛 파워
  defense         Int     // 수비
  stamina         Int     // 스테미나
  rarity          String  // 등급

  @@map("players")
}
